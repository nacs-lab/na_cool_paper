issn = {0031-9007},
journal = {Phys. Rev. Lett.},
keywords = {Broken Tag,MQM,Non-EDM Symmetry Violation},
mendeley-tags = {Broken Tag,MQM,Non-EDM Symmetry Violation},
month = {sep},
number = {10},
pages = {103003},
title = {{Time-Reversal Symmetry Violation in Molecules Induced by Nuclear Magnetic Quadrupole Moments}},
