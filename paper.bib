keywords = {BioCARS,Broken Tag,Chiral monolayer,Chirality,Circular Dichroism,Electromagnetic Fields,Molecular chirality,Naphthols,Naphthols: chemistry,Nonlinear circular dichroism,Nonlinear optical activity,Nonlinear optics,Optical Rotation,Optically active liquid,Optics and Photonics,Raman,Review,SFG,SHG,Second-harmonic generation,Spectroscopy,Spectrum Analysis,Stereoisomerism,Sum-frequency generation},
mendeley-tags = {Broken Tag,Chirality},
month = {oct},
number = {8},
pages = {421--437},
pmid = {16082658},
title = {{Nonlinear optical spectroscopy of chiral molecules}},
