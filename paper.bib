month = {apr},
number = {14},
pages = {144301},
pmid = {21495749},
title = {{He-ThO(1$\Sigma$+) interactions at low temperatures: elastic and inelastic collisions, transport properties, and complex formation in cold 4He gas.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21495749},
volume = {134},
year = {2011}
}
