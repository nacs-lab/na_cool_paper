@article{Tarbutt2015NJP,
abstract = {Laser cooling and magneto-optical trapping of molecules typically involves multiple transitions driven by several laser frequencies. We analyze how magneto-optical trapping forces depend on the angular momenta, F l and F u , and the g -factors, g l and g u , of the lower and upper states. When {\#}{\#}IMG{\#}{\#} [http://ej.iop.org/images/1367-2630/17/1/015007/njp506467ieqn1.gif] {\{}{\$}{\{}{\{}F{\}}{\_}{\{}{\{}\backslashrm l{\}}{\}}{\}}\backslashgt {\{}{\{}F{\}}{\_}{\{}{\{}\backslashrm u{\}}{\}}{\}}{\$}{\}} the polarizations must be reversed relative to cases where {\#}{\#}IMG{\#}{\#} [http://ej.iop.org/images/1367-2630/17/1/015007/njp506467ieqn2.gif] {\{}{\$}{\{}{\{}F{\}}{\_}{\{}{\{}\backslashrm u{\}}{\}}{\}}\backslashgeqslant {\{}{\{}F{\}}{\_}{\{}{\{}\backslashrm l{\}}{\}}{\}}.{\$}{\}} The correct choice of circular polarization depends on the sign of g u but not on the sign of g l . If g u is zero there is no trapping force, and the trapping force is very weak whenever g u is small compared to g l , which it usually is when the cooling transition is the {\#}{\#}IMG{\#}{\#} [http://ej.iop.org/images/1367-2630/17/1/015007/njp506467ieqn3.gif] {\{}{\$}{\^{}}{\{}2{\}}\backslashSigma {\$}{\}} to {\#}{\#}IMG{\#}{\#} [http://ej.iop.org/images/1367-2630/17/1/015007/njp506467ieqn4.gif] {\{}{\$}{\^{}}{\{}2{\}}{\{}{\{}\backslashPi {\}}{\_}{\{}1/2{\}}{\}}{\$}{\}} transition of a molecule. For some molecules, mixing of the excited {\#}{\#}IMG{\#}{\#} [http://ej.iop.org/images/1367-2630/17/1/015007/njp506467ieqn5.gif] {\{}{\$}{\^{}}{\{}2{\}}{\{}{\{}\backslashPi {\}}{\_}{\{}1/2{\}}{\}}{\$}{\}} state with a nearby {\#}{\#}IMG{\#}{\#} [http://ej.iop.org/images/1367-2630/17/1/015007/njp506467ieqn6.gif] {\{}{\$}{\^{}}{\{}2{\}}\backslashSigma {\$}{\}} excited state can greatly increase g u , leading to stronger trapping forces. A strong trapping force can also be produced by rapidly and synchronously reversing both the magnetic field and the laser polarizations. We simulate a recent experiment on magneto-optical trapping of SrF molecules, and suggest that an alternative choice of laser beam polarizations will strengthen the trapping force.},
author = {Tarbutt, M. R.},
doi = {10.1088/1367-2630/17/1/015007},
