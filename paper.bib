journal = {J. Chem. Phys.},
keywords = {Broken Tag,ThO Spectroscopy},
mendeley-tags = {Broken Tag,ThO Spectroscopy},
number = {2},
pages = {798--805},
title = {{Relativistic and correlated calculations on the ground and excited states of ThO}},
volume = {119},
year = {2003}
}
