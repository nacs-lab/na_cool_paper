publisher = {Nature Publishing Group},
title = {{Local charge of the $\nu$ = 5/2 fractional quantum Hall state.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21228871},
volume = {469},
year = {2011}
}
