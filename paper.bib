file = {:C$\backslash$:/Users/NickHutzler/Dropbox/Papers/Desorption of alkali atoms and ions from oxide surfaces Relevance to origins of Na and K in atmospheres of Mercury and the Moon - Madey.pdf:pdf},
issn = {0148-0227},
