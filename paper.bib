@article{Murphy2002,
abstract = {We present a short review of the current quasar (QSO) absorption line constraints on possible variation of the fine structure constant, alpha = e{\^{}}2/(hbar*c). Particular attention is paid to recent optical Keck/HIRES spectra of 49 absorption systems which indicate a smaller alpha in the past (Murphy et al. 2001, Webb et al. 2001). Here we present new preliminary results from 128 absorption systems: da/a = (-0.57 +/- 0.10) x 10{\^{}}{\{}-5{\}} over the redshift range 0.2 {\textless} z {\textless} 3.7, in agreement with the previous results. Known potential systematic errors cannot explain these results. We compare them with strong `local' constraints and discuss other (radio and millimetre-wave) QSO absorption line constraints on variations in alpha{\^{}}2 * g{\_}p and alpha{\^{}}2 * g{\_}p * m{\_}e/m{\_}p (g{\_}p is the proton g-factor and m{\_}e/m{\_}p is the electron/proton mass ratio). Finally, we discuss future efforts to rule out or confirm the current 5.7 sigma optical detection.},
archivePrefix = {arXiv},
arxivId = {astro-ph/0209488},
author = {Murphy, M. T. and Webb, J. K. and Flambaum, V. V. and Curran, S. J.},
