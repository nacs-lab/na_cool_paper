title = {{Electrostatic trapping of ammonia molecules}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10952305{\%}5Cnhttp://www.nature.com/nature/journal/v406/n6795/full/406491A0.html},
volume = {406},
